<div class="container">
	<div class="text-center">
		<h1>Add New Venue</h1>
	</div>
		<div class="text-center">

			<%= bootstrap_form_for(@venue, layout: :vertical) do |f|%>
				<label><%= f.text_field :name %></label>
				<label><%= f.text_field :city %></label>
				<label><%= f.text_field :state %></label>
				<label><%= f.text_field :family_friendly %></label>
		<div class="text-center">
			<%= submit_tag "Add" %>
		</div>
		<% end %>
	</div>
</div>